function sumarTradicional(a, b) {
  return a + b;
}

const sumarFlecha = (a, b) => a + b;

const resultadoTradicional = sumarTradicional(5, 3);
console.log("Resultado tradicional:", resultadoTradicional);

const resultadoFlecha = sumarFlecha(10, 2);
console.log("Resultado flecha:", resultadoFlecha);